{
   "name": "pmmp/pocketmine-mp",
   "description": "A server software for Minecraft: Pocket Edition written in PHP",
   "type": "project",
   "homepage": "https://pmmp.io",
   "license": "LGPL-3.0",
   "require": {
      "php": ">=7.2.0RC3",
      "ext-bcmath": "*",
      "ext-curl": "*",
      "ext-hash": "*",
      "ext-json": "*",
      "ext-mbstring": "*",
      "ext-openssl": "*",
      "ext-pcre": "*",
      "ext-phar": "*",
      "ext-pthreads": ">=3.1.7dev",
      "ext-reflection": "*",
      "ext-sockets": "*",
      "ext-spl": "*",
      "ext-yaml": ">=2.0.0",
      "ext-zip": "*",
      "ext-zlib": ">=1.2.11",
      "pmmp/raklib": "dev-master#e46352f9df4f34f70dd40b8a3144e941ffc1caf7",
      "pmmp/pocketmine-spl": "^0.1.0"
   },
   "autoload": {
      "psr-0": {
         "": ["src"]
      }
   },
   "repositories": [
      {
         "type": "vcs",
         "url": "https://github.com/pmmp/RakLib"
      },
      {
         "type": "vcs",
         "url": "https://github.com/pmmp/PocketMine-SPL"
      }
   ]
}
